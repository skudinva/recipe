generator client {
  provider = "prisma-client-js"
  //  output   = "../src/_generator"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TaxPayers {
  id         String   @id
  building   String?
  city       String?
  district   String?
  house      String?
  index      String
  inn        String
  office     String?
  ogrn       String
  okato      String?
  okfs       String?
  okopf      String?
  okpo       String?
  okved2     String?
  region     String
  settlement String?
  shortName  String
  statusCode String
  statusDate DateTime
  street     String
}
